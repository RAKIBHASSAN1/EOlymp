#include<bits/stdc++.h>
using namespace std;
int main()
{
     string s;
     cin>>s;
     int k=0,k1=0;
     for(int i=1;i<s.length();i++)
     {
         if(s[i]=='+')
         {
             k1++;
         }

         if(s[i]=='-')
         {
             k1++;
         }
         if(s[i]=='*')
         {
             k1++;
         }

     }
     k=k1;
     cout<<k<<endl;
}
